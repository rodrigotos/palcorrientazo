<table class="table">
  <tr>
    <th>Nombre</th>
    <th>Zona</th>
    <th>Precio</th>
    <th>Estrellas</th>
    <th>Likes</th>
    <th>Calificame</th>
    <th>Te gusto ?</th>
  </tr>
</table>
<table class="table table-striped">
  <% establishments.each do |e| %>
  <tr>
    <td><strong><%= e.name %></strong></td>
    <td><strong><%= e.zone %></strong></td>
    <td><strong><%= e.price %></strong></td>
    <td><strong><%= e.avg_qualification %></strong></td>
    <td><strong><%= e.like_count %></strong></td>
    <td><button type="button" class="btn btn-primary" name="button">Calificar</button></td>
    <td><button type="button" class="btn btn-primary" name="button">Like</button></td>
  </tr>
  <% end %>
</table>





